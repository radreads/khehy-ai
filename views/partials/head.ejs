<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta name="description" content="<%= typeof metaDescription !== 'undefined' ? metaDescription : 'Join Khe Hy on his AI learning journey, from Wall Street to Creator Economy to AI innovation.' %>">
<meta name="keywords" content="Khe Hy, AI, Technology, Learning, Wall Street, Creator Economy">
<meta name="author" content="Khe Hy">
<meta name="robots" content="index, follow">

<!-- Open Graph / Facebook -->
<meta property="og:type" content="website">
<meta property="og:url" content="<%= typeof currentUrl !== 'undefined' ? currentUrl : 'https://yoursite.com' %>">
<meta property="og:title" content="<%= typeof title !== 'undefined' ? title + ' - Khe Hy' : 'Khe Hy' %>">
<meta property="og:description" content="<%= typeof metaDescription !== 'undefined' ? metaDescription : 'Join Khe Hy on his AI learning journey, from Wall Street to Creator Economy to AI innovation.' %>">
<meta property="og:image" content="/images/og-image.jpg">

<!-- Twitter -->
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:site" content="@khemaridh">
<meta name="twitter:title" content="<%= typeof title !== 'undefined' ? title + ' - Khe Hy' : 'Khe Hy' %>">
<meta name="twitter:description" content="<%= typeof metaDescription !== 'undefined' ? metaDescription : 'Join Khe Hy on his AI learning journey, from Wall Street to Creator Economy to AI innovation.' %>">
<meta name="twitter:image" content="/images/og-image.jpg">

<link rel="canonical" href="<%= typeof currentUrl !== 'undefined' ? currentUrl : 'https://yoursite.com' %>">
